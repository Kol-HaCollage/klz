{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"jsx": "react-jsx",
		"types": [
			"vitest/globals",
			"vitest/importMeta",
			"vite/client",
			"node",
			"vitest",
			"@nx/react/typings/cssmodule.d.ts",
			"@nx/react/typings/image.d.ts"
		],
		"outDir": "./out-tsc/vitest"
	},
	"references": [
		{
			"path": "./tsconfig.app.json"
		}
	],
	"include": [
		"vite.config.ts",
		"vite.config.mts",
		"vitest.config.ts",
		"vitest.config.mts",
		"src/**/*.test.ts",
		"src/**/*.spec.ts",
		"src/**/*.test.tsx",
		"src/**/*.spec.tsx",
		"src/**/*.test.js",
		"src/**/*.spec.js",
		"src/**/*.test.jsx",
		"src/**/*.spec.jsx",
		"src/**/*.d.ts"
	]
}
